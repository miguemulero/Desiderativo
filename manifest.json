{
  "manifest_version": 3,
  "name": "Cuestionario Desiderativo",
  "version": "2.0",
  "description": "An√°lisis integral del Cuestionario Desiderativo",
  "permissions": [
    "storage",
    "tabs"
  ],
  "host_permissions": [
    "https://notebooklm.google.com/*"
  ],
  "action": {
    "default_title": "Abrir Cuestionario Desiderativo"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://notebooklm.google.com/*"],
      "js": ["notebooklm-content.js"],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "128": "icon.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["form.html", "form.css", "form.js"],
      "matches": ["<all_urls>"]
    }
  ]
}